   "source": [
    "## copy public key\n",
    "\n",
    "```bash\n",
    "$pub = Get-Content $env:USERPROFILE\\.ssh\\id_ed25519.pub\n",
    "ssh user@xxx.xxx.xxx.xxx \"mkdir -p ~/.ssh && chmod 700 ~/.ssh && echo '$pub' >> ~/.ssh/authorized_keys && chmod 600 ~/.ssh/authorized_keys\"\n",
    "```"
   ]
  }
